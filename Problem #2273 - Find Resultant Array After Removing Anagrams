from collections import Counter
class Solution:
    def removeAnagrams(self, words: List[str]) -> List[str]:
        dic=dict()
        n=len(words)
        i=0
        while i<n:
            if dic!=Counter(words[i]):
                dic=Counter(words[i])
                i+=1
            else:
                del(words[i])
                n-=1
        return words
